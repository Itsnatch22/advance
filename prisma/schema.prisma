datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Calculation {
  id          String   @id @default(uuid())
  grossPay    Decimal
  netPay      Decimal
  deductions  Json
  country     String?
  city        String?
  ipAddress   String
  device      String?
  createdAt   DateTime @default(now())

  @@map("wage_calculations")
}
